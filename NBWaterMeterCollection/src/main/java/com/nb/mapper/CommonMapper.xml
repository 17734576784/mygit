<mapper namespace="com.nb.mapper.CommonMapper" >
   
 <select id="getNbInfoByDeviceId" parameterType="string" resultType="map">
 	 select rtu_id rtuId,mp_id mpId from nb_water_meter 
 	 where device_id = #{deviceId,jdbcType=VARCHAR}
 </select>
 
 
